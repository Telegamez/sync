# =============================================================================
# VOICE AI PROVIDER CONFIGURATION (FEAT-1000-1006)
# =============================================================================

# Voice AI Provider Selection
# Options: "openai" (default) | "xai"
# This controls which provider is used for real-time voice-to-voice AI.
# Other AI services (transcription, summaries) continue to use OpenAI.
VOICE_AI_PROVIDER=your_vendor_here

# OpenAI API Key (required when VOICE_AI_PROVIDER=openai)
# Used for: Realtime voice AI, transcription (Whisper), summaries (GPT-4o-mini)
OPENAI_API_KEY=your_key_here

# XAI API Key (required when VOICE_AI_PROVIDER=xai)
# Get your key from: https://x.ai/api
XAI_API_KEY=your_key_here

# Port for the application
PORT=your_port_here

# =============================================================================
# SUPABASE CONFIGURATION
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
# =============================================================================

# Project URL (from Settings > API > Project URL)
NEXT_PUBLIC_SUPABASE_URL=your_key_here

# Anon/Public Key (from Settings > API > anon public)
# Safe for client-side use
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_key_here

# Service Role Key (from Settings > API > service_role)
# SERVER-SIDE ONLY - Never expose to client!
SUPABASE_SERVICE_ROLE_KEY=your_key_here

SERPER_API_KEY=your_key_here

YOUTUBE_API_KEY=your_key_here

SERPER_API_KEY=your_key_here

YOUTUBE_API_KEY=your_key_here
